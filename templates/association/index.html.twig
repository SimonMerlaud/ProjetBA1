{% extends 'base.html.twig' %}

{% block h1 %}
    Liste des associations
{% endblock %}

{% block body %}
    <div class="justify-content-md-end text-center">
        <form action="{{ url("association_add") }}" >
            <button class="btn btn-secondary " type="submit" >Ajouter une association</button>
        </form>
        <div class="row d-flex justify-content-center">
            <div class="col-4 mt-2">
                {{ render(controller(
                    'App\\Controller\\SearchController::searchAsso'
                )) }}
            </div>
        </div>
    </div>
    {{ include('association/list.html.twig', {'associations': associations,'nbPage': nbPage,'currentPage': currentPage}) }}



{% endblock %}
