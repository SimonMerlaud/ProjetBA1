<form method="post" action="{{ path('booking_delete', {'id': booking.id, 'magId':magId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ booking.id) }}">
    <button class="btn btn-lg btn-danger mx-1 ">Supprimer</button>
</form>